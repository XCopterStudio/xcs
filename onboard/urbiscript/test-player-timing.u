echo("Started");
load("init_onboard.u");



/*
 * Initialize nodes
 */
var Global.player = XDataplayer.new("logs/timing.txt");
var player = Global.player;
player.flag = 0;
at(player.flag == 1) {
    echo("Flag = 1");
} onleave {
    echo("Flag = 0");
};

player.command = "Play";
player.command = "Pause";
sleep(5.0s);
player.command = "Play";


at(player.finished) {
    echo("Finished");
};
