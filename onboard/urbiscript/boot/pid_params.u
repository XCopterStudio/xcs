class PidParams {
    var p = 0.0;
    var i = 0.0;
    var d = 0.0;
    var sumI = 0.0;
    var lastValue = 0.0;
    var lastDerivation = 0.0;
    var timeDelay = 0.0;

    function init(p,i,d) {
        this.p = p;
        this.i = i;
        this.d = d;
    };

    function reset() {
        this.sumI = 0.0;
        this.lastValue = 0.0;
        this.lastDerivation = 0.0;
    };
}|;
