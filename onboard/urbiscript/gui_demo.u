/*
 * GUI demo
 */

var xci = XXci.new("xci_dodo");
var f = ULineFinder.new();

xci.&video >> f.&video;

serverConnect("127.0.0.1", "1234", xci);

xci.xciInit();

xci.setConfiguration("video:filename", "video.mp4");
xci.setConfiguration("video:fps", "50");
xci.doCommand("Load");
xci.doCommand("Play");

// Default values for image processing
f.hsvValueRange = 10;
f.houghRho = 1;
